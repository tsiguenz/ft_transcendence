set number
set relativenumber
syntax on
colorscheme ron
set colorcolumn=0
set tabstop=4
set shiftwidth=4
inoremap jj <Esc>
set splitbelow
set splitright
set listchars=tab:>-
set hlsearch
set wildmenu
set wildignorecase
let mapleader=" "
nnoremap <leader>h <C-W>h
nnoremap <leader>j <C-W>j
nnoremap <leader>k <C-W>k
nnoremap <leader>l <C-W>l
